    "eslint-config-next": "13.5.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "lucide-react": "^0.331.0",
    "next": "^14.1.0",
    "postcss": "8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/node": "20.6.3",
    "@types/react": "18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11"
